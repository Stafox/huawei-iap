  "type": "library",
  "authors": [
    {
      "name": "Stas Kuryan",
      "email": "stas.kuryan@gmail.com",
      "homepage": "https://github.com/Stafox"
    }
  ],
  "keywords": [
    "huawei",
    "hms",
    "in app",
    "purchase",
    "subscription",
    "store",
    "iap"
  ],
